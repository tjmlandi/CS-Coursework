Prim
%%%
    public abstract Val eval();
%%%

AddPrim
%%%
    public Val eval() {
	return PrimVal.addPrim;
    }

    public String toString() {
	return PrimVal.addPrim.toString();
    }
%%%

SubPrim
%%%
    public Val eval() {
	return PrimVal.subPrim;
    }

    public String toString() {
	return PrimVal.subPrim.toString();
    }
%%%

MulPrim
%%%
    public Val eval() {
	return PrimVal.mulPrim;
    }

    public String toString() {
	return PrimVal.addPrim.toString();
    }
%%%

DivPrim
%%%
    public Val eval() {
	return PrimVal.divPrim;
    }

    public String toString() {
	return PrimVal.divPrim.toString();
    }
%%%

Add1Prim
%%%
    public Val eval() {
	return PrimVal.add1Prim;
    }

    public String toString() {
	return PrimVal.add1Prim.toString();
    }
%%%

Sub1Prim
%%%
    public Val eval() {
	return PrimVal.sub1Prim;
    }

    public String toString() {
	return PrimVal.sub1Prim.toString();
    }
%%%

ZeropPrim
%%%
    public Val eval() {
	return PrimVal.zeropPrim;
    }

    public String toString() {
	return PrimVal.zeropPrim.toString();
    }
%%%
